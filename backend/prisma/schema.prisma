generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Producto {
  id          Int     @id @default(autoincrement())
  codigo      String  @unique
  nombre      String
  descripcion String
  precio      Float
  stock       Int
  categoria   String
  imagenes    String
  bloqueado   Boolean @default(false)
}